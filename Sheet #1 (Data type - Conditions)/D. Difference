
/*
Given four numbers A, B, C and D. Print the result of the following equation :

 X = (A * B) - (C * D).
 */
 
declare
aa number;
bb number;
cc number;
dd number;
x number ;
y number;
temp number;



begin

aa :='&aa';
bb :='&bb';
cc :='&cc';
dd :='&dd';
x := aa * bb;
y := cc * dd;
temp :=x - y ;

dbms_output.put_line('Difference = '||temp);
end;
-- input: 1 2 3 4

/*
Difference = -10
*/


